{
  "name": "mahiru",
  "version": "2025.120.0",
  "description": "A Discord bot.",
  "main": "dist/main.js",
  "scripts": {
    "analyze-code": "eslint \"src/**\"",
    "build": "tsc --build tsconfig.json",
    "build-windows": "rmdir dist /s /q & npm run build",
    "build-linux": "rm -rf dist & npm run build",
    "format-code": "prettier -w --loglevel=silent \"src/**\"",
    "start": "node --trace-warnings .",
    "startDebug": "node --trace-warnings --inspect .",
    "startRecalculation": "node --trace-warnings dist/recalculation.js",
    "type-check": "tsc --noEmit",
    "update-osu-libraries-beta": "npm i @rian8337/osu-base@beta @rian8337/osu-difficulty-calculator@beta @rian8337/osu-droid-replay-analyzer@beta @rian8337/osu-droid-utilities@beta @rian8337/osu-rebalance-difficulty-calculator@beta @rian8337/osu-strain-graph-generator@beta",
    "update-osu-libraries-release": "npm i @rian8337/osu-base @rian8337/osu-difficulty-calculator @rian8337/osu-droid-replay-analyzer @rian8337/osu-droid-utilities @rian8337/osu-rebalance-difficulty-calculator @rian8337/osu-strain-graph-generator",
    "use-local-osu-module": "npm i ../osu-droid-module/packages/osu-base ../osu-droid-module/packages/osu-difficulty-calculator ../osu-droid-module/packages/osu-droid-replay-analyzer ../osu-droid-module/packages/osu-droid-utilities ../osu-droid-module/packages/osu-rebalance-difficulty-calculator ../osu-droid-module/packages/osu-strain-graph-generator"
  },
  "packageManager": "pnpm@10.29.1",
  "engines": {
    "node": ">=18.x"
  },
  "keywords": [
    "discordjs",
    "typescript",
    "osudroid",
    "osu",
    "osu!",
    "osu!droid",
    "discord.js"
  ],
  "type": "commonjs",
  "author": "Rian8337",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/adm-zip": "^0.5.7",
    "@types/d3": "^6.7.8",
    "@types/node": "^22.19.10",
    "@types/yt-search": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "@google/genai": "^1.40.0",
    "@rian8337/osu-base": "4.0.0-beta.86",
    "@rian8337/osu-difficulty-calculator": "4.0.0-beta.86",
    "@rian8337/osu-droid-replay-analyzer": "4.0.0-beta.86",
    "@rian8337/osu-droid-utilities": "4.0.0-beta.86",
    "@rian8337/osu-rebalance-difficulty-calculator": "4.0.0-beta.86",
    "adm-zip": "^0.5.16",
    "bufferutil": "^4.1.0",
    "canvas": "^3.2.1",
    "consola": "^3.4.2",
    "d3": "^6.7.0",
    "discord.js": "^14.25.1",
    "dotenv": "^16.6.1",
    "erlpack": "github:discord/erlpack",
    "html-entities": "^2.6.0",
    "module-alias": "^2.3.4",
    "mongodb": "^7.1.0",
    "mysql2": "^3.16.3",
    "sodium-native": "^5.0.10",
    "utf-8-validate": "^6.0.6",
    "words-count": "^2.0.2",
    "youtube-dl-exec": "3.0.13",
    "yt-search": "^2.13.1",
    "zlib-sync": "^0.1.10"
  },
  "_moduleAliases": {
    "@core": "dist/core",
    "@database": "dist/database",
    "@enums": "dist/enums",
    "@events": "dist/events",
    "@interactions": "dist/interactions",
    "@localization": "dist/localization",
    "@structures": "dist/structures",
    "@utils": "dist/utils"
  }
}
